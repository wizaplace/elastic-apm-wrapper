{
    "name": "wizaplace/elastic-apm-wrapper",
    "description": "Wizaplace Elastic APM Wrapper",
    "keywords": [
        "Elastic APM"
    ],
    "homepage": "https://github.com/wizaplace/elastic-apm-wrapper",
    "license": "Proprietary",
    "authors": [
        {
            "name": "Wizacha DevTeam",
            "email": "dev@wizacha.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "monolog/monolog": "^1",
        "wizaplace/elastic-apm-php-agent": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Wizacha\\ElasticApm\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Wizacha\\ElasticApm\\Tests\\": "/Tests/"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/wizaplace/elastic-apm-php-agent.git"
        }
    ],
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "infection/infection": "^0.15.3",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12.25",
        "phpstan/phpstan-deprecation-rules": "^0.12.4",
        "phpstan/phpstan-strict-rules": "^0.12.2",
        "phpunit/phpunit": "^7",
        "squizlabs/php_codesniffer": "^3.5"
    }
}

{
    "name": "wizaplace/elastic-apm-wrapper",
    "description": "Wizaplace Elastic APM Wrapper",
    "keywords": [
        "Elastic APM"
    ],
    "homepage": "https://github.com/wizaplace/elastic-apm-wrapper",
    "license": "Proprietary",
    "authors": [
        {
            "name": "Wizacha DevTeam",
            "email": "dev@wizacha.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "monolog/monolog": "^1",
        "philkra/elastic-apm-php-agent": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Wizacha\\ElasticApm\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Wizacha\\ElasticApm\\Tests\\": "/Tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "phpstan/phpstan": "^0.11.19",
        "friendsofphp/php-cs-fixer": "^2.16",
        "squizlabs/php_codesniffer": "^3.5"
    }
}
